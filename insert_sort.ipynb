    "def insertion_sort(a):\n",
    "    for i in range(1, len(a)):\n",
    "        cur_val = a[i]\n",
    "        j = i - 1\n",
    "        while j >= 0 and cur_val < a[i]:\n",
    "            a[j+1] = a[j]\n",
    "            j -= 1\n",
    "\n",
    "\n",
    "A = [1, 3, 5, 124, 0, -223, 44, 146]\n",
    "insertion_sort(A)\n",
    "print(A)\n"
   ]
  }
